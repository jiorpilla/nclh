
<table class="table">
    <thead>
    <tr>
        <th>Date</th>
        <th>Full Name</th>
    </tr>
    </thead>
    <tbody>
    {% for list in appointment_lists %}
        <tr>
            <td>{{ list.appointmentDate |date('Y-m-d') }}</td>
            <td>
                {{ list.appointee.lastName }}, {{ list.appointee.firstName }} {{ list.appointee.middleName|first }}., {{ list.appointee.suffix }}
            </td>
        </tr>
    {% else %}
        <tr>
            <td colspan="25">no records found</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<div id="pagination-container">
    {{ knp_pagination_render(appointment_lists) }}
</div>